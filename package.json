{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "run-p next-dev start-db",
    "build": "next build",
    "start": "run-p next-start start-db",
    "lint": "next lint",
    "test": "jest",
    "next-start": "next start",
    "next-dev": "next dev",
    "create-db": "cp jsonServer/initialDB.json jsonServer/db.json",
    "watch-db": "json-server -c jsonServer/json-server.json --watch jsonServer/db.json",
    "start-db": "run-s create-db watch-db",
    "prettier": "prettier --write .",
    "precommit": "run-s lint prettier"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@mdx-js/react": "1",
    "@theme-ui/color": "^0.14.7",
    "json-server": "^0.17.0",
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "theme-ui": "^0.14.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "18.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}
